citHeader("To cite the CoImp package in publications use:")

bibentry(bibtype="Manual",
         title ="CoImp: Parametric and Nonparametric Copula-Based Imputation Methods",
         author = as.person("F. Marta L. Di Lascio [aut], Aurora Gatto [aut], Simone Giannerini [aut]"), 
         organization = " Free University of Bozen-Bolzano",        
         address      = " Bozen-Bolzano, Italy",
         year         = 2025,
         note         = "R package version 2.1.0",
         url          = "https://CRAN.R-project.org/package=CoImp",
         textVersion =  
         paste("Di Lascio F.M.L., Gatto A., Giannerini S. (2025) ", 
               "CoImp: Parametric and Nonparametric Copula-Based Imputation Methods, ",
               "Free University of Bozen-Bolzano, Italy ",
                "https://CRAN.R-project.org/package=CoImp",
                " Version = 2.1.0",".",sep="") )
               
